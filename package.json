{
  "name": "icode",
  "displayName": "iCode - iOS Development Tools",
  "description": "iOS development tools for VS Code/Cursor: build, run, and manage iOS projects",
  "version": "1.1.2",
  "publisher": "nsleader",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Debuggers"
  ],
  "activationEvents": [
    "workspaceContains:**/*.xcodeproj/project.pbxproj",
    "workspaceContains:**/*.xcworkspace/contents.xcworkspacedata"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "icode.selectScheme",
        "title": "iCode: Select Scheme"
      },
      {
        "command": "icode.selectSimulator",
        "title": "iCode: Select Simulator/Device"
      },
      {
        "command": "icode.selectConfiguration",
        "title": "iCode: Select Configuration (Debug/Release)"
      },
      {
        "command": "icode.build",
        "title": "iCode: Build Project"
      },
      {
        "command": "icode.run",
        "title": "iCode: Build & Run"
      },
      {
        "command": "icode.configureIndex",
        "title": "iCode: Configure Swift Index (SourceKit-LSP)"
      },
      {
        "command": "icode.resolvePackages",
        "title": "iCode: Resolve SPM Dependencies"
      },
      {
        "command": "icode.configureDebug",
        "title": "iCode: Configure Debug (LLDB)"
      }
    ],
    "configuration": {
      "title": "iCode",
      "properties": {
        "icode.build.skipMacroValidation": {
          "type": "boolean",
          "default": true,
          "description": "Skip Swift macro validation for faster builds (Xcode 15+)"
        },
        "icode.build.parallelizeTargets": {
          "type": "boolean",
          "default": true,
          "description": "Build independent targets in parallel"
        },
        "icode.build.disableAutoPackageResolution": {
          "type": "boolean",
          "default": false,
          "description": "Disable automatic SPM package resolution (use 'Resolve SPM Dependencies' command manually)"
        }
      }
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
